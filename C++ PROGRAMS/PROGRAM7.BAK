#include<iostream.h>
#include<conio.h>
//Operator Overloading program.
class complex
{
int a,b;
public:
complex()
{
a=0;
b=0;
}
complex(int x,int y)
{
a=x;
b=y;
}
complex operator +(complex c)
{
complex ob;
ob.a=a+c.a;
ob.b=b+c.b;
return ob;
}
};
